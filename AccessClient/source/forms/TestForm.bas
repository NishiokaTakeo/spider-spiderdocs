Public Function VersionIdByDocId(docid As String) As Collection
 On Error GoTo ErrCatcher
   
    If client Is Nothing Then
        Call MsgBox("You need create instance for client", vbOKOnly, "Not good. Not good.")
        Exit Function
    End If
        
    '
    ' Search Document
    '
    Dim Criteria As Dictionary: Set Criteria = New Dictionary
    
    If docid <> "" Then
        Criteria("DocIds") = Array(docid) 'by document's ids
    End If
        
    Dim histories As Object: Set histories = client.GetHistories(Criteria)
     
    Dim id_versions As Collection: Set id_versions = New Collection
    For Each history In histories
    
        If id_versions.Count = 0 Then
        
            id_versions.add history("id_version")
            
        Else
        
            'Add if id_version is not in the list
            Dim i As Integer: i = 1
            Dim add As Boolean: add = True
            
            For i = 1 To id_versions.Count
                
                If id_versions(i) = history("id_version") Then
                    add = False
                    Exit For
                End If
                
            Next
            
            If add Then
                id_versions.add history("id_version")
            End If
            
        End If
        
    Next
    
    client.Dispose
    
    Call DisActive
    
    Set VersionIdByDocId = id_versions
    
    Exit Function
    
ErrCatcher:
     
    Resume Next
End Function

