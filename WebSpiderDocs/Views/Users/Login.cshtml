				<h1>Spider Docs Login</h1>

			</header>
			<p style="text-align: center; background: red; font-weight: bold; color: white;" class="hidden">NEW STYLE will apply</p>

			@using (Html.BeginForm("LoginByForm", "Users", FormMethod.Post, new { enctype = "multipart/form-data" }))
			{

    			@Html.AntiForgeryToken()

				<span id="lblError" style="color:Orange;font-size:Small;">@Html.ValidationSummary()</span>
				<div class="form-field">
					<label for="txtUsername" class="form-field__label">@Html.LabelFor(a => a.Login)</label>
					@Html.TextBoxFor(a => a.Login, new { @class = "form-control", @autocomplete = "off", @placeholder = "Login ID" })

					<div class="form-field">
						<label for="txtPassword" class="form-field__label">@Html.LabelFor(a => a.Password)</label>
						@Html.TextBoxFor(a=>a.Password,new{ @class="form-control",@autocomplete="off",@placeholder="Password",type="Password"})

					</div>
					@* <div class="form-field form-field--checkbox hidden">
						<input id="accountremember" type="checkbox" name="accountremember" checked="checked">
						<label for="accountremember" class="form-field__label">Remember me</label>
					</div>
					, new { enctype = "multipart/form-data" }
					 *@

					<div class="login-form__submit">
						<a href="forgot-password.aspx" class=""></a>
						<input type="submit" value="Login" class="btn btn--blue" style="background-color: #2666B9; color: #fff;">
					</div>

				</div>
			}

			</div>

			<!-- <script src="scripts/app/build.js"></script> -->

			<script>
				$('[name="accountremember"] input').unwrap();
				sessionStorage.clear();
				localStorage.clear();
			</script>

